<?xml version="1.0"?>
<!DOCTYPE table SYSTEM "TableModel.dtd">

<table>
	<key-column property-name="id"/>
	<column property-name="tradeId" header-label="Trade">
	</column>
	<column property-name="stornoGroupId" header-label="Deal">
	</column>
	<column property-name="instrument" header-label="Instrument">
	</column>
	<column property-name="structure" header-label="Structure">
	</column>
	<column property-name="tradeType" header-label="Type">
	</column>
	<column property-name="bookId" header-label="Book">
	</column>
	<column property-name="counterparty" header-label="Counterparty">
	</column>
	<column property-name="traderId" header-label="Trader">
	</column>
	<column property-name="traderLocation" header-label="Location">
	</column>
	<column property-name="reportLocation" header-label="Local Report">
	</column>
	<column property-name="tradeDate" header-label="Trade time">
		<date-format type="timelong"/>
	</column>
	<column property-name="systemDate" header-label="Amend time">
		<date-format type="timelong"/>
	</column>
	<column property-name="currentPriceDifference" header-label="Price difference" column-type="number">
		<number-format fraction-min-length="4" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="priceTolerance" header-label="Price tolerance" column-type="number">
		<number-format fraction-min-length="4" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="tradePrice" header-label="NPV" column-type="number">
		<number-format fraction-min-length="4" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="amendmentNpvChange" header-label="NPV Change" column-type="number">
		<number-format fraction-min-length="4" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="automaticStateCode" header-label="Automatic state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.AutoStateCodeList"/>
	</column>
	<column property-name="manualStateCode" header-label="Manual state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.ManualStateCodeList"/>
	</column>
	<column property-name="manualStateComment" header-label="Manual comment">
	</column>
	<column property-name="reclamationStateCode" header-label="Reclamation state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.ReclamationStateCodeList"/>
	</column>	
</table>
