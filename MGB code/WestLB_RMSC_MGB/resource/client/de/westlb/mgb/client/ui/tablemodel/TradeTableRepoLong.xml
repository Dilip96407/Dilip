<?xml version="1.0"?>
<!DOCTYPE table SYSTEM "TableModel.dtd">

<table>
	<key-column property-name="id"/>
	<column property-name="tradeId" header-label="Trade">
	</column>
	<column property-name="isin" header-label="ISIN">
	</column>
	<column property-name="instrument" header-label="Instrument">
	</column>
	<column property-name="underlyingValueGroup" header-label="Category">
	</column>
	<column property-name="underlyingType" header-label="SubType">
	</column>
	<column property-name="tradeDate" header-label="Trade time">
		<date-format type="timelong"/>
	</column>
	<column property-name="currency" header-label="Currency">
	</column>
	<column property-name="bookId" header-label="Portfolio">
	</column>
	<column property-name="traderLocation" header-label="Location">
	</column>
	<column property-name="reportLocation" header-label="Local Report">
	</column>
	<column property-name="volume" header-label="Start cash" column-type="number">
		<number-format fraction-min-length="2" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="counterparty" header-label="Counterparty">
	</column>
	<column property-name="repoRate" header-label="Repo rate / Fee / Rate" column-type="number">
		<number-format fraction-min-length="2" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="marketPrice" header-label="Repo market price" column-type="number">
		<number-format fraction-min-length="2" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="basePointDifference" header-label="BPoint difference" column-type="number">
		<number-format fraction-min-length="0" fraction-max-length="0" grouping-length="3"/>
	</column>
	<column property-name="basePointTolerance" header-label="BPoint tolerance" column-type="number">
		<number-format fraction-min-length="0" fraction-max-length="0" grouping-length="3"/>
	</column>
	<column property-name="profitLossEffect" header-label="P/L-effect" column-type="number">
		<number-format fraction-min-length="2" fraction-max-length="2" grouping-length="3"/>
	</column>
	<column property-name="startDate" header-label="Start time">
		<date-format type="datelong"/>
	</column>
	<column property-name="endDate" header-label="End time">
		<date-format type="datelong"/>
	</column>
	<column property-name="automaticStateCode" header-label="Automatic state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.AutoStateCodeList"/>
	</column>
	<column property-name="manualStateCode" header-label="Manual state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.ManualStateCodeList"/>
	</column>
	<column property-name="reclamationStateCode" header-label="Reclamation state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.ReclamationStateCodeList"/>
	</column>
</table>
