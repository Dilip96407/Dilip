<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.westlb.mgb.model.impl.RequestImpl" table="T06_REQUEST" discriminator-value="DEF" batch-size="50">
		<!--
    	<cache usage="read-write"/>
    	-->
 		<id name="longId" type="long" column="T06_ID">
			<generator class="sequence">
                <param name="sequence">HIB_SEQ</param>			
			</generator>
		</id>
		<discriminator column="T06_TYPE" length="3" />
		<many-to-one name="addon" lazy="proxy" column="FK_T06_T13_ADDON" class="de.westlb.mgb.model.impl.AddonImpl"/>
		<many-to-one name="trade" lazy="proxy" class="de.westlb.mgb.model.impl.TradeImpl" >
			<column name="FK_T06_T02_TRADE" index="I06_TRADE"/>
		</many-to-one>
		<many-to-one name="priceCheckCategory" lazy="proxy" column="FK_T06_T11_PRICE_CHECK_CAT" class="de.westlb.mgb.model.impl.PriceCheckCategoryImpl"/>
        <property name="requestState" column="T06_REQUEST_STATE"/>
        <property name="requestType" column="T06_REQUEST_TYPE" length="20"/>
        <property name="requestDate" column="T06_REQUEST_DATE"/>
        <property name="requestString" column="T06_REQUEST_STRING"/>
        <many-to-one name="priceResult" lazy="proxy" class="de.westlb.mgb.model.impl.PriceImpl" cascade="save-update,delete" unique="true">
        	<column name="FK_T06_T12_PRICE_RESULT" index="I06_PRICE_RESULT" />
		</many-to-one>
        <many-to-one name="mandant" lazy="proxy" class="de.westlb.mgb.model.impl.MandantImpl">
			<column name="FK_T06_T09_MANDANT" sql-type="VARCHAR2(3)"/>
		</many-to-one>
        <property name="changedDate" column="T06_CHANGED_DATE"/>
 		<property name="changedBy" column="T06_CHANGED_BY" length="10"/>
		<property name="createdBy" column="T06_CREATED_BY" length="10"/>
        <property name="creationDate" column="T06_CREATION_DATE" />

		<subclass name="de.westlb.mgb.model.impl.BloombergRequestImpl" discriminator-value="BLB" batch-size="50">
	        <property name="requestField" column="T06_REQUEST_FIELD" length="50"/>
	        <property name="requestSources" column="T06_REQUEST_SOURCES" length="255"/>
		</subclass>

		<subclass name="de.westlb.mgb.model.impl.ReutersRequestImpl" discriminator-value="RIC" batch-size="50">
		</subclass>

		<subclass name="de.westlb.mgb.model.impl.EuwaxRequestImpl" discriminator-value="EUW" batch-size="50">
		</subclass>

		<subclass name="de.westlb.mgb.model.impl.FoRequestImpl" discriminator-value="FOR" batch-size="50">
		</subclass>

    </class>

</hibernate-mapping>
