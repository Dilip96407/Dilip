<?xml version="1.0"?>
<!DOCTYPE table SYSTEM "TableModel.dtd">

<table>
	<column property-name="name" header-label="Name">
	</column>
	<column property-name="priceTolerancePercent" header-label="Relative tolerance (in %)">
		<number-format fraction-min-length="2" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="priceToleranceAbsolute" header-label="Absolute tolerance">
		<number-format fraction-min-length="2" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="timeToleranceMinutes" header-label="Time tolerance">
		<number-format fraction-min-length="0" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="manualSampleCode" header-label="Sample state code" >
	</column>
	<column property-name="manualSamplePercentage" header-label="Sample percentage" >
	</column>
	<column property-name="changePending" header-label="Pending change" column-type="boolean">
	</column>
	<column property-name="changeRequestedByName" header-label="Changed by">
	</column>
</table>
