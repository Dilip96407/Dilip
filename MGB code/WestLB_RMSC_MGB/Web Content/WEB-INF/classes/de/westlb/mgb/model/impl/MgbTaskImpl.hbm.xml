<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.westlb.mgb.model.impl.MgbTaskImpl" table="T63_MGB_TASK" lazy="false">
		<!--
    	<cache usage="read-write"/>
    	-->
 		<id name="longId" type="long" column="T63_ID">
			<generator class="sequence">
                <param name="sequence">HIB_SEQ</param>			
			</generator>
		</id>
		<many-to-one name="mandant" lazy="proxy" class="de.westlb.mgb.model.impl.MandantImpl">
			<column name="FK_T63_T09_MANDANT" sql-type="VARCHAR2(3)"/>
		</many-to-one>
		<property name="taskName" column="T63_TASK_NAME"/>
		<property name="threadName" column="T63_THREAD_NAME"/>
		<property name="user" column="T63_USER"/> 
		<property name="startTime" column="T63_START_DATE"/>
		<property name="stopTime" column="T63_STOP_DATE"/>
		<property name="message" column="T63_MESSAGE"/>
		<property name="state" column="T63_STATE"/>
		<property name="client" column="T63_CLIENT"/>

        <property name="changedDate" column="T63_CHANGED_DATE"/>
 		<property name="changedBy" column="T63_CHANGED_BY" length="10" />  
		<property name="createdBy" column="T63_CREATED_BY" length="10" /> 
        <property name="creationDate" column="T63_CREATION_DATE" />
    </class>

</hibernate-mapping>
