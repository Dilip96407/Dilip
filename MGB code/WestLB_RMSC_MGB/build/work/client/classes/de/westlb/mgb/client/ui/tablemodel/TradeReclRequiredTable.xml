<?xml version="1.0"?>
<!DOCTYPE table SYSTEM "TableModel.dtd">

<table>
	<column property-name="tradeId" header-label="Trade">
	</column>
	<column property-name="employeeName" header-label="Employee name">
	</column>
	<column property-name="email" header-label="E-Mail">
	</column>
	<column property-name="phone" header-label="Phone">
	</column>
	<column property-name="manualStateCode" header-label="Manual state">
		<choice-list list-class="de.westlb.mgb.client.ui.selection_list.ManualStateCodeList"/>
	</column>
	<column property-name="lateEntry" header-label="Late entry" column-type="boolean">
	</column>
	<column property-name="sourceSystemInstrument" header-label="Instrument">
	</column>
	<column property-name="systemDate" header-label="System time">
		<date-format type="timelong"/>
	</column>
	<column property-name="settlementDate" header-label="Settlement time">
		<date-format type="timelong"/>
	</column>
	<column property-name="tradeDate" header-label="Trade time">
		<date-format type="timelong"/>
	</column>
	<column property-name="volume" header-label="Volume" column-type="number">
		<number-format fraction-min-length="4" fraction-max-length="10" grouping-length="3"/>
	</column>
	<column property-name="currency" header-label="Currency">
	</column>
	<column property-name="price" header-label="Price" column-type="number">
		<number-format fraction-min-length="4" fraction-max-length="10" grouping-length="3"/>
	</column>
</table>
