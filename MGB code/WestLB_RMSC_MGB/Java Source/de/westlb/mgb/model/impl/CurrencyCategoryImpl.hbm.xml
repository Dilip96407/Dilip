<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.westlb.mgb.model.impl.CurrencyCategoryImpl" table="T51_CURRENCY_CATEGORY" lazy="false">
    	<cache usage="read-write"/>
     	<composite-id>
	        <key-property name="code" column="T51_ID" length="25"/>
			<key-many-to-one name="mandant" lazy="false" class="de.westlb.mgb.model.impl.MandantImpl" column="FK_T51_T09_MANDANT" />
     	</composite-id>
        <property name="name" column="T51_NAME" length="50"/>
        <property name="changedDate" column="T51_CHANGED_DATE"/>
 		<property name="changedBy" column="T51_CHANGED_BY" length="10"/>
		<property name="createdBy" column="T51_CREATED_BY" length="10"/>
        <property name="creationDate" column="T51_CREATION_DATE" />
    </class>

</hibernate-mapping>
